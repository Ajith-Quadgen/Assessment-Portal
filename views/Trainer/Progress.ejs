<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%- title %></title>
    <%- include('../partials/dependencies') %>
</head>
<% if(Role=="Admin" ){ %>
    <%- include('../partials/admin-header',{title:title}) %>
        <% }else if(Role=="Employee" ){ %>
            <%- include('../partials/employee-header',{title:title}) %>
                <%} if(Role=="Trainer" ){ %>
                    <%- include('../partials/trainer-header',{title:title}) %>
                        <%}%>

                            <body>
                                <div class="main_container">
                                    <% if (Object.values(Data).length>0) { %>
                                        <input type="hidden" name="AssessmentID" id="AssessmentID"
                                            value="<%= Data[0].Assessment_ID %>">
                                        <%}%>
                                            <div
                                                class="d-flex w-100  justify-content-center align-items-center flex-column">
                                                <table
                                                    class="table table-hover table-sm m-2 AVRTable rounded-4 overflow-hidden">
                                                    <thead class="table-dark">
                                                        <tr class="text-center">
                                                            <th scope="col"> Sl.No</th>
                                                            <th scope="col">User</th>
                                                            <th scope="col">Assessment</th>
                                                            <th scope="col">Start Time</th>
                                                            <th scope="col">End Time</th>
                                                            <th scope="col" colspan="2">Status</th>
                                                            <th></th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <% if (Object.values(Data).length>0) { %>

                                                            <% for( let index=0; index < Object.values(Data).length;
                                                                index++ ) { %>
                                                                <tr class="text-center">
                                                                    <td>
                                                                        <%= index+1%>
                                                                    </td>
                                                                    <td>
                                                                        <%= Data[index].employeeName %> <br><sub>(<%=
                                                                                    Data[index].User_ID %>)</sub>
                                                                    </td>
                                                                    <td>
                                                                        <%= Data[index].AssessmentName %> <br>
                                                                            <sub>(<%= Data[index].Assessment_ID %>
                                                                                    )</sub>
                                                                    </td>
                                                                    <td>
                                                                        <%= Data[index].S_Date %>
                                                                    </td>
                                                                    <td>
                                                                        <%= Data[index].E_Date %>
                                                                    </td>
                                                                    <td>
                                                                        <%= Data[index].Status %>
                                                                    </td>
                                                                    <td>
                                                                        <button type="button"
                                                                            class="btn btn-outline-danger border-0 btn-sm"
                                                                            onclick="deleteSession('<%=Data[index].idAssessment_Session%>')"><i
                                                                                class="bi bi-trash"></i></button>
                                                                    </td>
                                                                </tr>
                                                                <% } %>
                                                                    <% } else { %>
                                                                        <tr>
                                                                            <td colspan="9">
                                                                                <svg xmlns="http://www.w3.org/2000/svg"
                                                                                    style="display: none;">
                                                                                    <symbol id="check-circle-fill"
                                                                                        viewBox="0 0 16 16">
                                                                                        <path
                                                                                            d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z" />
                                                                                    </symbol>
                                                                                    <symbol id="info-fill"
                                                                                        viewBox="0 0 16 16">
                                                                                        <path
                                                                                            d="M8 16A8 8 0 1 0 8 0a8 8 0 0 0 0 16zm.93-9.412-1 4.705c-.07.34.029.533.304.533.194 0 .487-.07.686-.246l-.088.416c-.287.346-.92.598-1.465.598-.703 0-1.002-.422-.808-1.319l.738-3.468c.064-.293.006-.399-.287-.47l-.451-.081.082-.381 2.29-.287zM8 5.5a1 1 0 1 1 0-2 1 1 0 0 1 0 2z" />
                                                                                    </symbol>
                                                                                    <symbol
                                                                                        id="exclamation-triangle-fill"
                                                                                        viewBox="0 0 16 16">
                                                                                        <path
                                                                                            d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z" />
                                                                                    </symbol>
                                                                                </svg>
                                                                                <div class="alert alert-warning alert-dismissible fade show d-flex align-items-center"
                                                                                    role="alert">
                                                                                    <svg class="bi flex-shrink-0 me-2"
                                                                                        role="img" aria-label="Info:">
                                                                                        <use xlink:href="#info-fill" />
                                                                                    </svg>

                                                                                    <strong>Sorry...!ðŸ˜•</strong> Data Not Found
                                                                                    <button type="button"
                                                                                        class="btn-close"
                                                                                        data-bs-dismiss="alert"
                                                                                        aria-label="Close"></button>
                                                                                </div>
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td colspan="9" class="text-center">

                                                                                <a href="/login"
                                                                                    class="btn btn-outline-primary rounded-pill"><i
                                                                                        class="bi bi-arrow-left-circle-fill"></i>
                                                                                    Go Back To Home</a>

                                                                            </td>
                                                                        </tr>
                                                                        <% } %>
                                                    </tbody>
                                                </table>
                            </body>

                            <%- include('../partials/dependencies-js') %>
                            <script>
                            
                                function deleteSession(id){
                                    axios.post("/api/deleteSession",{params:{
                                        ID:id
                                    }}).then(res=>{
                                    if(res.status==200){
                                        alert("The Session has been deleted successfully,Now this user can take up the assessment.")
                                        window.location.reload()
                                    }
                                    }).catch(e=>{
                                        console.log(e.response.data)
                                    })
                                }
                            </script>

</html>